class GFG {
        public int matrixSum(int arr[][]) { 
	    int ans = 0;
	    for(int i=0;i<arr.length;i++){
	        for(int j=0;j<arr.length;j++){
	            int no_of_times=(i+1)*(j+1)*(arr.length-i)*(arr.length-j);
                 ans+=arr[i][j]*no_of_times;
                
	        }
	    }
	    return ans;
    }
