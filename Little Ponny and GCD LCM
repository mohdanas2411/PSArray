public class Solution {
    public int[] solve(int a, int b) {
      int[] ans=new int[2];
      if(a==b){
         ans[0]=a;
         ans[1]=b;
      }
     else if(a%b==0 || b%a==0){
            ans[0]=a;
            ans[1]=b;  
     }
     else{
              ans[0]=-1;
              ans[1]=-1;
       }
       return ans;
    }
}
